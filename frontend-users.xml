<quiqqer>
    <frontend-users>
        <profile>

            <categories>
                <category name="sequry">

                    <title>
                        <locale group="sequry/template"
                                var="sequry.usersettings.category.title"/>
                    </title>

                    <!-- Address -->
                    <settings control="\QUI\FrontendUsers\Controls\Profile\Address"
